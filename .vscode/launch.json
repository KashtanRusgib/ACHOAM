{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run CHOAM Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"runtimeExecutable": "${execPath}",
			"skipFiles": [
				"<node_internals>/**"
			]
		}
	]
}
